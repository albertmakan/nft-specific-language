packages {
  using std.ERC20 as E
}

contract Brojko combines {
  E.approve
  E.transfer with "0x111111111111111111111111111111111111111" milos and 34% bc and "ABC" str and [
    0x1111111111111111111111111111111111111111,
    0xaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa
  ] owners
}

administration {
